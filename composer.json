{
    "name": "dhukuti/dolly",
    "description": "A test package using Russian Doll Caching technique in Laravel.",
    "license": "MIT",
    "keywords":["laravel","cache","dolly","laracast","test"],
    "authors": [
        {
            "name": "Niraj Byanjankar",
            "email": "newroseji@hotmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "illuminate/support": "^5.2"
    },
    "autoload": {
        "psr-4":{
            "Dhukuti\\Dolly\\":"src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "illuminate/database": "^5.2",
        "illuminate/cache": "^5.2"
    }
}
